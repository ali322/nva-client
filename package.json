{
  "name": "nva-client",
  "version": "1.2.5",
  "description": "gui client form nva",
  "main": "dist/main/index.js",
  "scripts": {
    "clean": "rimraf dist/renderer",
    "pack:osx": "cross-env BUILD_TARGET=darwin node script/packer.js",
    "pack:win": "cross-env BUILD_TARGET=win32 node script/packer.js",
    "pack": "npm run pack:osx && npm run pack:win",
    "publish": "electron-builder",
    "rebuild": "electron-rebuild -f -w node-sass",
    "start": "node script/runner.js",
    "build": "node script/builder.js"
  },
  "author": "alichen",
  "license": "MIT",
  "devDependencies": {
    "@types/decompress": "^4.2.3",
    "@types/form-data": "^2.2.1",
    "@types/fs-extra": "^5.0.5",
    "@types/lodash": "^4.14.122",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-transition-group": "^2.9.2",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.6",
    "@types/styled-components": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "core-decorators": "^0.20.0",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "decompress": "^4.2.1",
    "electron": "9.4.0",
    "electron-builder": "22.8.1",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^3.0.1",
    "electron-rebuild": "^1.8.5",
    "esbuild-loader": "^1.3.1",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "form-data": "^2.3.3",
    "get-stream": "^6.0.1",
    "globby": "^9.1.0",
    "got": "11.8.2",
    "ionicons": "^4.6.1",
    "mobx": "^5.10.0",
    "mobx-react": "^5.2.8",
    "mobx-sync": "^2.1.0",
    "normalize.css": "^8.0.1",
    "p-event": "^4.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.13.0",
    "react-router-cache-route": "^1.11.0",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.1.1",
    "rimraf": "^2.6.3",
    "semver": "^7.3.5",
    "styled-components": "^4.3.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "xterm": "^4.5.0",
    "xterm-addon-fit": "^0.3.0"
  },
  "dependencies": {
    "axios": "0.21.1",
    "chalk": "^2.4.2",
    "co": "^4.6.0",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "npminstall": "3.27.0",
    "nva-task": "1.0.57"
  },
  "build": {
    "productName": "NVA Client",
    "appId": "org.alilab.nva.client",
    "asar": false,
    "directories": {
      "app": ".",
      "output": "./output"
    },
    "files": [
      "./dist/**/*",
      "./term/**/*"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "./script/icons/nva.icns"
    },
    "win": {
      "target": [
        "NSIS",
        "zip"
      ],
      "icon": "./script/icons/nva.ico",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 150,
          "type": "file"
        },
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "nsis": {
      "perMachine": true,
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": [
      {
        "provider": "github",
        "repo": "nva-client",
        "owner": "ali322",
        "releaseType": "release"
      }
    ]
  }
}
